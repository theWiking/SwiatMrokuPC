<Page
    x:Class="SwiatMrokuPC.NowaKarta"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SwiatMrokuPC"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.Resources>
        <ResourceDictionary>
            <Style TargetType="Button" x:Key="StylGowny">
                <Setter Property="FontSize"
                                Value="30"/>
                <Setter Property="FontStyle"
                            Value="Normal"/>
                <Setter Property="Foreground"
                        Value="Black"/>
                <Setter Property="Width"
                        Value="300"/>
                <Setter Property="Height"
                        Value="50"/>
            </Style>
            <Style x:Key="TextBoxStyle1" TargetType="TextBox">
                <Setter Property="BorderBrush"
                        Value="Black"/>
                <Setter Property="Background" 
                        Value="Yellow"/>
                <Setter Property="HorizontalAlignment"
                        Value="Left"/>
                <Setter Property="VerticalAlignment"
                        Value="Top"/>
                <Setter Property="MaxHeight"
                        Value="1"/>
                <Setter Property="Width"
                        Value="150"/>
                <Setter Property="MaxLength"
                        Value="50"/>
                <Setter Property="Margin"
                        Value="5,0,0,0"/>


            </Style>
            <Style x:Key="TextBoxNumeric" TargetType="TextBox">
                <Setter Property="BorderBrush"
                                Value="Black"/>
                <Setter Property="Background"
                                Value="White"/>
                <Setter Property="InputScope"
                                Value="Number"/>
                <Setter Property="MaxHeight"
                                Value="1"/>
                <Setter Property="Width"
                                Value="1"/>
                <Setter Property="MaxLength"
                                Value="1"/>
                <Setter Property="HorizontalAlignment"
                        Value="Right"/>
                <Setter Property="VerticalAlignment"
                        Value="Top"/>
                <Setter Property="Margin"
                        Value="0,0,20,0"/>
            </Style>
            <Style x:Key="BorderStyle" TargetType="Border">
                <Setter Property="BorderBrush"
                        Value="Black"/>
                <Setter Property="BorderThickness"
                        Value="3"/>

            </Style>
            <Style x:Key="NumeryczneWpisywanie" TargetType="TextBox">
                <Setter Property="BorderBrush"
                                Value="Black"/>
                <Setter Property="Background"
                                Value="White"/>
                <Setter Property="InputScope"
                                Value="Number"/>
                <Setter Property="MaxHeight"
                                Value="1"/>
                <Setter Property="Width"
                                Value="150"/>
                <Setter Property="MaxLength"
                                Value="7"/>
                <Setter Property="HorizontalAlignment"
                        Value="Left"/>
                <Setter Property="VerticalAlignment"
                        Value="Top"/>
            </Style>

            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key ="Dark">
                    <Style BasedOn="{StaticResource StylGowny}"
                           TargetType="Button">
                        <Setter Property="Foreground"
                                Value="DarkGreen"/>
                    </Style>
                </ResourceDictionary>
                <ResourceDictionary x:Key ="Light">
                    <Style BasedOn="{StaticResource StylGowny}"
                           TargetType="Button">
                        <Setter Property="Foreground"
                                Value="DeepSkyBlue"/>
                    </Style>
                </ResourceDictionary>
                <ResourceDictionary x:Key="BorderStyleHard">
                    <Style BasedOn="{StaticResource BorderStyle}"
                           TargetType="Border">
                        <Setter Property="BorderThickness"
                                Value="3"/>
                    </Style>
                </ResourceDictionary>


            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Page.Resources>

